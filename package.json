{
  "name": "CasperDash",
  "version": "1.1.7",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14 Pro Max'",
    "start": "react-native start",
    "ios:mainnet": "yarn env:mainnet && react-native run-ios",
    "android:mainnet": "yarn env:mainnet && react-native run-android",
    "ss": "watchman watch-del-all && react-native start --reset-cache",
    "c": "adb reverse tcp:8081 tcp:8081",
    "format": "prettier --write **/*.{ts,tsx,js,json,md}",
    "check-code": "prettier --check **/*.{ts,tsx,js,json,md}",
    "apk": "cd android && ./gradlew clean && ./gradlew app:assembleRelease && cd ..",
    "aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "encode-ios": "react-native bundle --dev false --entry-file index.js --bundle-output ios/main.jsbundle --platform ios",
    "encode-android": "react-native bundle --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --platform android --assets-dest android/app/src/main/res/",
    "test": "jest --coverage",
    "lint": "eslint .",
    "env:mainnet": "node scripts/set-env.js mainnet",
    "env:testnet": "node scripts/set-env.js testnet",
    "env:beta": "node scripts/set-env.js beta",
    "env:local": "node scripts/set-env.js local",
    "setup-env": "yarn env:mainnet && yarn env:testnet",
    "fastlane:beta": "yarn env:beta && cd ios/fastlane && fastlane beta",
    "fastlane:release": "yarn env:mainnet && cd ios/fastlane && fastlane beta",
    "fastlane:beta:android": "yarn env:beta && cd android/fastlane && fastlane internal",
    "fastlane:release:android": "yarn env:mainnet && cd android/fastlane && bundle exec fastlane internal --verbose",
    "postinstall": "rn-nodeify --install process,stream,crypto --hack --yarn"
  },
